/* ARENAS TORRES JUAN EDUARDO
#include <stdio.h>

int main() {
    float a, b;
    printf("Ingresa el primer numero: ");
    scanf("%f", &a);

    printf("Ingresa el segundo numero: ");
    scanf("%f", &b);

    printf("Suma: %.2f\n", a + b);
    printf("Resta: %.2f\n", a - b);
    printf("Multiplicacion: %.2f\n", a * b);

    return 0;
}
